<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="ar.com.rjgodoy.plan111mil.torneo.modelo.Competidor" table="competidores">
  <id name="id">
    <generator class="identity"></generator>
  </id>
  
  <property name="dni"/>   
  <property name="nombres"/>
  <property name="apellido"/>
  <property name="direccion"/>
  <property name="fechaNacimiento" column="fecha_nacimiento" type="date"/>
 	
  <many-to-one name="escuela" class="ar.com.rjgodoy.plan111mil.torneo.modelo.Escuela">
  	<column name="escuela"/>
  </many-to-one>
  
 </class>
</hibernate-mapping>
